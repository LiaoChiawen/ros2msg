#straps_msgs

# See section 6.4 of ICD

uint8 TRAUMA_HEAD=4
uint8 TRAUMA_TORSO=5
uint8 TRAUMA_LOWER_EXT=6
uint8 TRAUMA_UPPER_EXT=7
uint8 ALERTNESS_OCULAR=8
uint8 ALERTNESS_VERBAL=9
uint8 ALERTNESS_MOTOR=10

uint8 TRAUMA_NORMAL=0
uint8 TRAUMA_WOUND=1
uint8 TRAUMA_AMPUTATION=2

uint8 OCULAR_OPEN=0
uint8 OCULAR_CLOSED=1
uint8 OCULAR_NOT_TESTABLE=2

uint8 ALERTNESS_NORMAL=0
uint8 ALERTNESS_ABNORMAL=1
uint8 ALERTNESS_ABSENT=2
uint8 ALERTNESS_NOT_TESTABLE=3

builtin_interfaces/Time stamp

int32 casualty_id
string system
uint8 type	# should be either TRAUMA_HEAD, TRAUMA_TORSO, TRAUMA_LOWER_EXT, TRAUMA_UPPER_EXT, ALERTNESS_OCULAR, ALERTNESS_VERBAL, ALERTNESS_MOTOR
uint8 value	# should be set according to the following rules:
# if type is TRAUMA_HEAD or TRAUMA_TORSO
#   value should be TRAUMA_NORMAL or TRAUMA_WOUND
# if type is TRAUMA_LOWER_EXT or TRAUMA_UPPER_EXT
#   value should be TRAUMA_NORMAL, TRAUMA_WOUND or TRAUMA_AMPUTATION
# if type is ALERTNESS_OCULAR
#   value should be OCULAR_OPEN, OCULAR_CLOSED or OCULAR_NOT_TESTABLE
# if type is ALERTNESS_VERBAL or ALERTNESS_MOTOR
#   value should be ALERTNESS_NORMAL, ALERTNESS_ABNORMAL, ALERTNESS_ABSENT or ALERTNESS_NOT_TESTABLE

float32 confidence

